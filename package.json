    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.34.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vitest": "^4.0.16"
